let userInput = prompt ("Введите любое число:"); // Запрос у пользователя  число
let number = +userInput; // Приводим строку к числу
let square = number * number; // Вычисляем квадрат 
let cube = number * number * number; // Вычисляем куб
let message = `${number} \n ${square} \n ${cube}`; // Формируем сообщение для вывода
alert(message); // Выводим результат в alert
